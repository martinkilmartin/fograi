    "next": "^13.0.6",
    "next-pwa": "^5.5.2",
    "next-themes": "^0.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-query": "^3.39.3",
    "react-responsive": "^9.0.2",
    "react-window": "^1.8.9",
    "sendpulse-api": "^1.1.5",
    "sharp": "^0.30.4",
    "theme-change": "^2.0.2",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.12",
    "@next/eslint-plugin-next": "^12.1.6",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.2.0",
    "@types/jest": "^27.5.1",
    "@types/node": "20.3.3",
    "@types/react": "18.0.9",
    "@types/react-test-renderer": "^18.0.0",
    "@types/react-window": "^1.8.5",
    "@typescript-eslint/eslint-plugin": "^5.24.0",
