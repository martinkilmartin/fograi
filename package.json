    "@vercel/node": "^1.15.2",
    "autoprefixer": "^10.4.7",
    "babel-jest": "^28.1.0",
    "daisyui": "^2.15.0",
    "eslint": "8.15.0",
    "eslint-config-next": "^13.0.6",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-markdown": "^2.2.0",
    "eslint-plugin-react": "^7.29.4",
    "husky": "^8.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^28.1.0",
    "jest-watch-typeahead": "^1.1.0",
    "lint-staged": "^12.4.1",
    "postcss": "8.4.13",
    "prettier": "2.6.2",
    "react-test-renderer": "^18.1.0",
    "tailwindcss": "3.0.24",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4",
    "webpack": "^5.72.1"
  },
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn run type-check"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "yarn lint",
      "yarn format"
    ],
    "**/*.{md,mdx,json}": [
      "yarn lint",
      "yarn format"
    ]
  }
}
